{
    "Brigadista":{
        "nombre":{
            "type":"String",
            "required":true
        },
        "email":{
            "type": "String",
            "required": true,
            "unique": true
        },
        "implemento":[{
            "type":"Schema.ObjectID",
            "ref":"Implemento"
        }],
        "capacitacion":[{
            "type":"Schema.ObjectID",
            "ref":"Capacitacion"
        }]
    },

    "Encargado":{
        "nombre":{
            "type":"String",
            "required":true
        },
        "email":{
            "type": "String",
            "required": true,
            "unique": true
        }
    },

    "modifica":{
        "encargado":{
            "type":"Schema.ObjectID",
            "ref":"Encargado"
        },
        "implemento":{
            "type":"Schema.ObjectID",
            "ref":"Implemento"
        }
    },

    "Implemento":{
        "tipo":{
            "type":"Schema.ObjectID",
            "ref":"tipo"
        },
        "estado":{
            "type":"Schema.ObjectID",
            "ref":"estado"
        },
        "fecha de vencimiento":{
            "type": "date",
            "required":true
        },
        "categoria":{
            "type":"Schema.ObjectID",
            "ref":"categoria"
        }
    },

    "tipo":{
        "nombre":{
            "type":"String",
            "required":true
        }
    },

    "Categoria":{
        "nombre":{
            "type":"String",
            "required":true
        }
    },

    "Estado":{
        "nombre":{
            "type":"String",
            "required":true
        }
    },

    "Capacitacion":{
        "nombre":{
            "type":"String",
            "required":true
        }
    }

}